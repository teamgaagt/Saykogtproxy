RED="\e[31m"
GREEN="\e[32m"
ENDCOLOR="\e[0m"

clear
echo -e "${GREEN}Installing Proxy...${ENDCOLOR}"
sleep 1
if [ -f "saykogt" ]; then
    echo -e "${RED}Deleting old proxy...${ENDCOLOR}"
    rm shift
    sleep 1
    echo -e "${GREEN}Getting proxy...${ENDCOLOR}"
fi
wget -q https://github.com/teamgaagt/Saykogtproxy/raw/main/saykogt
sleep 1
echo -e "${GREEN}Proxy Installed${ENDCOLOR}"
echo -e "${GREEN}Execute proxy with this command: ./saykoproxy${ENDCOLOR}"
chmod +x saykoproxy
